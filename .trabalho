import androidx.annotation.NonNull;
import androidx.annotation.Nullable;
import androidx.room.ColumnInfo;
import androidx.room.Entity;
import androidx.room.PrimaryKey;
@Entity(tableName = "courses")
public class Course {
    @PrimaryKey(autoGenerate = true)
    @NonNull
    @ColumnInfo(name = "curseId")
    private int rId;
    @Nullable
    @ColumnInfo(name = "Establishment")
    private String rEstablishment;
    
    public int getId() {
        return rId;
    }
    public void setId(@NonNull int id) {
        rId = id;
    }
    public String getEstablishment() {
        return rEstablishment;
    }
    public void setEstablishment(@Nullable String establishment) {
        rEstablishment = establishment;
    }
    public String getTitle() {
        return rTitle;
    }
    public void setTitle(String title) {
        rTitle = title;
    }
    public String getCreationDate() {
        return rCreationDate;
    }
    public void setCreationDate(String creationDate) {
        rCreationDate = creationDate;
    }
    private String rTitle;
    private String rCreationDate;
    public int getGrade() {
        return rGrade;
    }
    public void setGrade(int grade) {
        rGrade = grade;
    }
    private int rGrade;
    public Course(String title, String  Establishment, int Grade, String creationDate) {
        rTitle = title;
        rEstablishment =  Establishment;
        rGrade = Grade;
        rCreationDate = creationDate;
    }
}
